{
  "name": "website-builder",
  "version": "1.0.0",
  "description": "Static website builder with realtime changes",
  "main": "server.js",
  "author": {
    "name": "Ricardo Pereira",
    "url": "https://github.com/rjcpereira/"
  },
  "private": true,
  "scripts": {
    "dev": "gulp dev",
    "build": "gulp",
    "server": "node server",
    "start": "gulp && npm run server",
    "compose": "docker-compose up --build",
    "dockerize": "npm run docker && npm run docker:start",
    "docker": "npm run docker:clear && npm run docker:build",
    "docker:clear": "docker rm -fv $(docker stop $(docker ps -a -q --filter ancestor=example --format=\"{{.ID}}\")) || true && docker rmi example -f",
    "docker:build": "docker build -t example .",
    "docker:start": "docker run -d -p 3000:3000 example",
    "_dockerize": "npm run docker && npm run docker-bo",
    "docker-bo": "cd bo && npm run docker"
  },
  "dependencies": {
    "@babel/preset-env": "^7.16.11",
    "axios": "^0.26.1",
    "del": "^6.0.0",
    "express": "^4.17.3",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-browserify": "^0.5.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-footer": "^2.1.0",
    "gulp-header": "^2.0.9",
    "gulp-json-minify": "^1.2.2",
    "gulp-minify": "^3.1.0",
    "gulp-nodemon": "^2.5.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-sass": "^5.1.0",
    "gulp-shell": "^0.8.0",
    "gulp-uglify": "^3.0.2",
    "handlebars": "^4.7.7",
    "mongodb": "^4.4.1",
    "sass": "^1.49.9",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1"
  }
}
